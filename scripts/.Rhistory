time_seriespeak<-
timeseries %>%
subset((seconds) >=6) %>%
subset((seconds) <= 8)
test<-lmer(pMTGL_univariatecluster~older_younger + (1|SUBID), data=time_seriespeak) #random intercepts
summary(test)
